{
  "name": "climedo-health",
  "version": "1.0.0",
  "description": "Identity management system",
  "main": "index.js",
  "author": "folabibanjo@gmail.com",
  "license": "MIT",
  "scripts": {
    "start": "node ./dist/server",
    "start:dev": "nodemon ./dist/server | bunyan -l debug",
    "start:debug": "nodemon --inspect ./dist/server | bunyan -l debug",
    "watch:tsc": "rm -rf ./dist && tsc --watch -p ./tsconfig.json",
    "build:tsc": "rm -rf ./dist && tsc -p ./tsconfig.json",
    "compile": "tsc",
    "start:worker": "node ./dist/server/workers/index",
    "start:worker:dev": "nodemon ./dist/server/workers/index --ignore ./dist/server/index | bunyan",
    "start:worker:debug": "nodemon --inspect ./dist/server/workers/index --ignore ./dist/server/index | bunyan",
    "test": "rm -f logs.json && NODE_ENV=test jest --runInBand --forceExit"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@hapi/joi-date": "^2.0.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.22.0",
    "bcrypt": "^5.0.1",
    "bunyan": "^1.8.15",
    "cors": "^2.8.5",
    "date-fns": "^2.25.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-request-id": "^1.4.1",
    "global": "^4.4.0",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "inversify": "^5.1.1",
    "inversify-express-utils": "^6.3.2",
    "joi": "^17.4.2",
    "lodash": "^4.17.21",
    "mockttp": "^2.2.4",
    "module-alias": "^2.2.2",
    "mongoose": "^6.0.10",
    "mongoose-autopopulate": "^0.16.0",
    "morgan": "^1.10.0",
    "prom-client": "^14.0.0",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "typescript": "^4.4.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bunyan": "^1.8.7",
    "@types/date-fns": "^2.6.0",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/faker": "^5.5.8",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.2",
    "@types/joi": "^17.2.3",
    "@types/lodash": "^4.14.175",
    "@types/morgan": "^1.9.3",
    "@types/redis": "^2.8.32",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "faker": "^5.5.3",
    "jest": "^27.2.5",
    "nodemon": "^2.0.13",
    "supertest": "^6.1.6",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1"
  },
  "_moduleAliases": {
    "@app": "./dist"
  }
}
